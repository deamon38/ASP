<!DOCTYPE html>
@{ var data = (Dictionary<int, RssItem[]>)ViewBag.News; }
<header id="header-wrap">
    <nav class="navbar navbar-expand-lg fixed-top scrolling-navbar indigo">
        <div class="container">
            <div class="navbar-header">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-navbar" aria-controls="main-navbar" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                    <span class="icon-menu"></span>
                    <span class="icon-menu"></span>
                    <span class="icon-menu"></span>
                </button>
            </div>
            <div class="collapse navbar-collapse" id="main-navbar">
                <ul class="navbar-nav mr-auto w-100 justify-content-center clearfix">
                    <li class="nav-item active">
                        <a class="nav-link" href="#carousel">
                            Home
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#zainteresowania">
                            Zainteresowania
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#news">
                            Newsy
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#contact">
                            Adres
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- Navbar End -->
    <!-- Hero Area Start -->
    <div id="carousel">
        <div class="container">
            <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="https://images.unsplash.com/photo-1504384308090-c894fdcc538d?ixlib=rb-1.2.1&ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&auto=format&fit=crop&w=1350&q=80" alt="First slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://images.unsplash.com/photo-1534665482403-a909d0d97c67?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80" alt="Second slide">
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://images.unsplash.com/photo-1552308995-2baac1ad5490?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80" alt="Third slide">
                    </div>
                </div>
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>
    <!-- Hero Area End -->

</header>
<!-- Header Area wrapper End -->
<!-- About Section Start -->
<section id="zainteresowania" class="section-padding">
    <div class="container">
        <div class="row">
            <div class="col text-center">
                <img src="https://www.avenga.com/wp-content/uploads/2020/11/C-Sharp.png" width="200" height="100" />
            </div>
            <div class="col">
                <br />
                Programowanie w języku C#
            </div>
            <div class="col text-center">
                <img src="https://cdn2.iconfinder.com/data/icons/round-set-vol-2/120/gamepad-512.png" width="100" height="100" />
            </div>
            <div class="col">
                <br />
                Gry komputerowe
            </div>
        </div>
        <br />
        <div class="row">
            <div class="col text-center">
                <img src="./lib/spa-template/img/clipart780150.png" width="100" height="100" />
            </div>
            <div class="col">
                <br />
                Czytanie powieści fantastycznch
            </div>
            <div class="col text-center">
                <img src="https://www.pngjoy.com/pngl/106/2205626_cartoon-food-healthy-food-clipart-transparent-transparent-png.png" width="100" height="100" />
            </div>
            <div class="col">
                <br />
                Zdrowe odżywianie
            </div>
        </div>
    </div>
</section>
<!-- About Section End -->
